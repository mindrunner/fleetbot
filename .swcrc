{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": true
      "legacyDecorator": true,
      "useDefineForClassFields": false,
    },
    "target": "ESNext"
  },
  "module": {
    "type": "commonjs",
    "strict": true
  }
  "sourceMaps": true
}

